<u:WidgetBase
    x:Class="Hjmos.Lcdp.Plugins.CS6.Views.PassengerFlow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:u="clr-namespace:Hjmos.Lcdp.VisualEditor.Core.BaseClass;assembly=Hjmos.Lcdp.VisualEditor.Core"
    xmlns:views="clr-namespace:Hjmos.Lcdp.Plugins.CS6.Views" d:DesignHeight="400" d:DesignWidth="400"
    prism:ViewModelLocator.AutoWireViewModel="True" mc:Ignorable="d">
    <views:PercentSectionChart
        Colors="{Binding Colors}"
        Gap="6"
        Labels="{Binding Labels}"
        RowCount="3"
        Series="{Binding Series}" />
</u:WidgetBase>
